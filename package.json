{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-pattern '**/*.interface.ts' & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-pattern '**/*.interface.ts'",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-pattern '**/*.interface.ts' --fix",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,mdx}\" --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:create": "npx sequelize-cli migration:create --name",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:create": "npx sequelize-cli seed:create --name",
    "seed:down": "npx sequelize-cli db:seed:undo",
    "seed:down:all": "npx sequelize-cli db:seed:undo:all",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.857.0",
    "@aws-sdk/s3-request-presigner": "^3.857.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.8",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/swagger": "^11.0.5",
    "@types/multer": "^1.4.12",
    "bcrypt": "^5.1.1",
    "canvas": "^3.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.5.0",
    "google-auth-library": "^9.15.1",
    "handlebars": "^4.7.8",
    "image-size": "^1.2.0",
    "ioredis": "^5.4.2",
    "jimp": "^0.22.10",
    "merge-images": "^2.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "npm-check-updates": "^17.1.14",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.16.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.1.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.8",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
